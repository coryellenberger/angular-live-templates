  "bugs": {
    "url": "https://github.com/coryellenberger/angular-live-templates/issues"
  },
  "homepage": "https://github.com/coryellenberger/angular-live-templates#readme",
  "dependencies": {
    "gulp": "^3.9.1",
    "gulp-bower": "0.0.13"
  },
  "devDependencies": {
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "standard": "^7.1.2"
